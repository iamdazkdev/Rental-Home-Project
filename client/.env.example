# React App Environment Variables - Example Template
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=30000

# Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_MAX_FILES_PER_LISTING=10
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# UI Configuration
REACT_APP_PAGINATION_LIMIT=12

# App Configuration
REACT_APP_NAME=Dream Nest
REACT_APP_VERSION=1.0.0
REACT_APP_ENVIRONMENT=development

# Features Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=true
REACT_APP_ENABLE_HOT_RELOAD=true

# External Services (fill in your actual keys)
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_key_here
# REACT_APP_STRIPE_PUBLISHABLE_KEY=your_stripe_key_here
# REACT_APP_FIREBASE_API_KEY=your_firebase_key_here

# Upload Configuration
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_MAX_FILES_PER_LISTING=10
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/webp

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_PAGINATION_LIMIT=12

# Development Tools
REACT_APP_SHOW_DEV_TOOLS=true
REACT_APP_LOG_LEVEL=debug